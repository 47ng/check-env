{
  "name": "@47ng/check-env",
  "version": "1.2.1",
  "main": "dist/index.js",
  "description": "Check that the critical environment variables are set",
  "license": "MIT",
  "author": {
    "name": "Fran√ßois Best",
    "email": "francois@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/47ng/check-env"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "environment",
    "environment-variables"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && tsc",
    "size": "size-limit",
    "prepare": "yarn ci",
    "ci": "yarn test && yarn build && yarn size"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.5",
    "jest": "^24.8.0",
    "size-limit": "^1.3.5",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "size": "1 KB"
    }
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
